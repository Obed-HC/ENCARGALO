<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout6"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".tendero.Inicio_sesion.ui.Soporte.sp_frgMenuterminoscondiciones">

    <!-- TODO: Update blank fragment layout -->


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginEnd="20dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_margin="20dp">

        <Button
            android:id="@+id/btn_so_04_terminos_condiciones"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/sp_iconterminos_y_condiciones"
            android:textAllCaps="false"
            android:drawablePadding="10dp"
            android:textAlignment="textStart"
            android:text="@string/menu_soporte_terminos_condiciones_terminos"
            android:background="@drawable/soporte_details_background2"
            android:layout_marginTop="20dp"
            android:layout_margin="20dp"
            android:padding="10dp"
            android:drawableRight="@drawable/sp_iconflecha"
            android:paddingRight="@dimen/activity_margin"
            />

        <Button
            android:id="@+id/btn_so_04_politica_privacidad"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/sp_iconpolitica_de_privacidad"
            android:textAllCaps="false"
            android:text="@string/menu_soporte_terminos_condiciones_politicas"
            tools:text="@string/menu_soporte_terminos_condiciones_politicas"
            android:background="@drawable/soporte_details_background2"
            android:layout_marginTop="20dp"
            android:layout_margin="20dp"
            android:drawablePadding="10dp"
            android:textAlignment="textStart"
            android:padding="10dp"
            android:drawableRight="@drawable/sp_iconflecha"
            android:paddingRight="@dimen/activity_margin"/>

        <Button
            android:id="@+id/btn_so_04_produc_prohibidos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/sp_iconlista_de_productos_prohibidos"
            android:textAllCaps="false"
            android:text="@string/menu_soporte_terminos_condiciones_lista_negra"
            tools:text="@string/menu_soporte_terminos_condiciones_lista_negra"
            android:background="@drawable/soporte_details_background2"
            android:layout_marginTop="20dp"
            android:layout_margin="20dp"
            android:padding="10dp"
            android:drawablePadding="10dp"
            android:textAlignment="textStart"
            android:drawableRight="@drawable/sp_iconflecha"
            android:paddingRight="@dimen/activity_margin"/>
    </LinearLayout>


</LinearLayout>